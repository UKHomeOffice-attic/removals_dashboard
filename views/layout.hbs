{{{ topOfPage }}}
<!DOCTYPE html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />

    <title>{{ title }}</title>

    <script src="__/env.js"></script>
    <script src="js/all.js"></script>

    <!--[if gt IE 8]><!--><link href="{{{ assetPath }}}css/govuk-template.css?0.16.0" media="screen" rel="stylesheet" type="text/css" /><!--<![endif]-->

    <link href="{{{ assetPath }}}css/govuk-template-print.css?0.16.0" media="print" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="css/styles.css" />

    <!--[if gte IE 9]><!-->
      <link href="{{{ assetPath }}}css/fonts.css?0.16.0" media="all" rel="stylesheet" type="text/css" />
    <!--<![endif]-->

    <link rel="shortcut icon" href="{{{ assetPath }}}images/favicon.ico?0.16.0" type="image/x-icon" />

    <!-- Size for iPad and iPad mini (high resolution) -->
    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{{ assetPath }}}images/apple-touch-icon-152x152.png?0.16.0">
    <!-- Size for iPhone and iPod touch (high resolution) -->
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="{{{ assetPath }}}images/apple-touch-icon-120x120.png?0.16.0">
    <!-- Size for iPad 2 and iPad mini (standard resolution) -->
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="{{{ assetPath }}}images/apple-touch-icon-76x76.png?0.16.0">
    <!-- Default non-defined size, also used for Android 2.1+ devices -->
    <link rel="apple-touch-icon-precomposed" href="{{{ assetPath }}}images/apple-touch-icon-60x60.png?0.16.0">

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="{{{ assetPath }}}images/opengraph-image.png?0.16.0">

    {{{ head }}}
  </head>

  <body class="{{ bodyClasses }}">
    <script type="text/javascript">document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
        {{{ bodyStart }}}
    <div id="skiplink-container">
      <div>
        <a href="#content" class="skiplink">{{ skipLinkMessage }}</a>
      </div>
    </div>

    <div id="global-cookie-message">
        {{{ cookieMessage }}}
    </div>
    <!--end global-cookie-message-->

    <header role="banner" id="global-header" class="{{{ headerClass }}}">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a href="{{{ homepageUrl }}}" title="{{ logoLinkTitle }}" id="logo" class="content">
              <img src="{{{ assetPath }}}images/gov.uk_logotype_crown_invert_trans.png?0.16.0" width="35" height="31" alt=""> {{{ globalHeaderText }}}
            </a>
          </div>
          {{{ insideHeader }}}
        </div>
        <div class="header-proposition">
          <div class="content">
            <a href="#proposition-links" class="js-header-toggle menu">Menu</a>
            <nav id="proposition-menu">
              <a href="{{{ homepageUrl }}}" id="proposition-name">{{ title }}</a>
              <ul id="proposition-links" class="">
                <li><a href="{{{ homepageUrl }}}" class="active">Bed availability</a></li>
                <li><a href="#statistics">Stats</a></li>
              </ul>
            </nav>
          </div>
        </div>
      </div>
    </header>
    <!--end header-->

    {{{ afterHeader }}}

    <div id="global-header-bar"></div>
    <!--end global-header-bar-->

    <main id="page-container" role="main">

      <div class="phase-banner">
        <p>
          <strong class="phase-tag">BETA</strong>
          <span>This is a new service â€“ your <a href="#">feedback</a> will help us to improve it.</span>
        </p>
      </div>

      <div id="content_container">
      </div>

    </main>

    <footer class="group js-footer" id="footer" role="contentinfo">
      <div class="footer-wrapper">
        {{{ footerTop }}}
      </div>
    </footer>
    <!--end footer-->

    <div id="global-app-error" class="app-error hidden"></div>
    {{{ bodyEnd }}}
  </body>
  <script src="{{{ assetPath }}}js/govuk-template.js?0.16.0" type="text/javascript"></script>
</html>
